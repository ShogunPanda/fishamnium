{
  "name": "fishamnium",
  "version": "4.3.2",
  "private": true,
  "description": "Some useful shell extension for fish shell.",
  "scripts": {
    "dev": "rollup -w -c rollup.config.js",
    "postdev": "chmod a+x bin/fishamnium",
    "prebuild": "rm -f bin/*",
    "build": "rollup --environment production -c rollup.config.js",
    "postbuild": "chmod a+x bin/fishamnium",
    "lint": "./node_modules/.bin/tslint --project tsconfig.json -t stylish --type-check",
    "postpublish": "git push origin && git push origin -f --tags",
    "old:release": "node utils/release",
    "old:postrelease": "git push origin && git push origin -f --tags",
    "old:convert-bookmarks": "node utils/convert-bookmarks",
    "old:generate-git-autocomplete": "FISHAMNIMUM_GENERATING_AUTOCOMPLETE=true node utils/generate-git-autocomplete > completions/71_fishamnium_helpers_git.fish",
    "old:local-install": "node utils/install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ShogunPanda/fishamnium"
  },
  "keywords": [],
  "author": "Shogun <shogun@cowtech.it>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ShogunPanda/fishamnium/issues"
  },
  "homepage": "https://sw.cowtech.it/fishamnium",
  "dependencies": {
    "chalk": "~2.3",
    "yargs": "~10.0"
  },
  "devDependencies": {
    "@cowtech/package-utils": "~0.2",
    "@cowtech/tslint-config": "~5.7",
    "@types/node": "~8.0",
    "@types/yargs": "~8.0",
    "rollup": "~0.49",
    "rollup-plugin-typescript2": "~0.5",
    "rollup-plugin-uglify": "~2.0",
    "ts-node": "~3.3",
    "typescript": "~2.5",
    "uglify-es": "~3.1"
  }
}
